{
  "name": "hawaii-js-root",
  "description": "Koa2 and Next.js full-stack, serverless-ready, monorepo boilerplate",
  "keywords": [
    "Koa2",
    "Next.js",
    "Serverless",
    "AWS",
    "AWS Lambda",
    "DynamoDB",
    "boilerplate"
  ],
  "repository": "https://github.com/rkrayovskyy/hawaii.js",
  "author": "Roman Krayovskyy <rkrayovskyy@gmail.com>",
  "license": "MIT",
  "private": true,
  "bin": {
    "prettier": "./node_modules/prettier/bin-prettier"
  },
  "scripts": {
    "server": "node ./cli/cmds/server",
    "client": "node ./cli/cmds/client",
    "docs": "node ./cli/cmds/docs",
    "db": "node ./cli/cmds/db",
    "repo": "node ./cli/cmds/repo",
    "sls": "node ./cli/cmds/sls",
    "test": "jest"
  },
  "workspaces": [
    "functions/*",
    "libs/*",
    "clients/*"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^14.0.1",
    "eslint-config-standard-jsx": "^8.0.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsdoc": "^15.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.0.0",
    "prettier": "^1.13.3",
    "shelljs": "^0.8.2",
    "yargonaut": "^1.1.4",
    "yargs": "^14.0.0"
  }
}
